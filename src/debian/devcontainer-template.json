{
  "id": "debian",
  "version": "1.0.0",
  "name": "Debian",
  "description": "Simple Debian container with Git installed.",
  "documentationURL": "https://github.com/devcontainers/templates/tree/main/src/debian",
  "options": {
    "imageVariant": {
      "type": "string",
      "description": "Debian version",
